# --- Cumulative Sum and Product ---
x <- c(2, 4, 6, 8, 10)

cat("Cumulative Sum:\n")
print(cumsum(x))

cat("Cumulative Product:\n")
print(cumprod(x))

# --- Minimum and Maximum ---
cat("Minimum Value:", min(x), "\n")
cat("Maximum Value:", max(x), "\n")

# --- Calculus ---
f <- function(x) { x^2 }

cat("Derivative of x^2:\n")
print(D(expression(x^2), "x"))

cat("Integration of x^2 from 0 to 3:\n")
print(integrate(function(x) x^2, 0, 3)$value)
